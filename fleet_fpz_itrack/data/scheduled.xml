<odoo>
    <data>
      <record id="itrack_cars_import" model="ir.cron">
        <field name="name">iTrack Car Import</field>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" >2016-12-31 00:30:00</field>
        <field name="model_id" ref="action_itrack_fleet_settings"/>
        <field name="function" eval="'import_cars_fms'" />
        <field name="priority" eval="5" />
      </record>
      <record id="itrack_cars_gps_import" model="ir.cron">
        <field name="name">iTrack GPS Sync</field>
        <field name="active" eval="True" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" >2016-12-31 00:40:00</field>
        <field name="model_id" ref="action_itrack_fleet_settings"/>
        <field name="function" eval="'import_gps_fms'" />
        <field name="priority" eval="7" />
      </record>
    </data>
</odoo>
